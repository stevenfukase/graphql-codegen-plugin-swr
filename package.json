{
  "name": "graphql-codegen-plugin-swr",
  "version": "0.0.9",
  "type": "module",
  "description": "A GraphQL code generator plug-in that automatically generates utility functions for SWR.",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stevenfukase/graphql-codegen-plugin-swr.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "graphql",
    "codegen",
    "typescript",
    "ts",
    "swr",
    "stale-while-revalidate",
    "react"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:main": "tsc -p tsconfig.main.json",
    "build:module": "tsc -p tsconfig.module.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint --fix",
    "test": "run-s build test:*",
    "test:lint": "eslint",
    "test:prettier": "prettier \"src/**/*.ts\" --list-different",
    "test:integrate": "jest --no-watchman",
    "watch:build": "tsc -p tsconfig.json -w"
  },
  "peerDependencies": {
    "graphql": "<17.0.0"
  },
  "dependencies": {
    "@graphql-codegen/plugin-helpers": "^5.1.0",
    "@graphql-codegen/visitor-plugin-common": "^5.6.0",
    "@types/micromatch": "^4.0.9",
    "graphql-request": "^7.1.2",
    "micromatch": "^4.0.8",
    "pascal-case": "^3.1.2",
    "swr": "^2.3.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@graphql-codegen/testing": "^3.0.4",
    "@graphql-codegen/typescript": "^4.1.2",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@graphql-codegen/typescript-operations": "^4.4.0",
    "@graphql-tools/apollo-engine-loader": "^8.0.13",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.6",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.14.0",
    "graphql": "^16.10.0",
    "jest": "^29.7.0",
    "jest-docblock": "^29.7.0",
    "jest-junit": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "files": [
    "build/main",
    "build/module",
    "!**/*.spec.*",
    "!**/*.json",
    "LICENSE",
    "README.md"
  ]
}
